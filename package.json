{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "push:prod": "dotenv -e .env.production -- npx prisma db push",
    "prepack": "husky install",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "emails": "email dev --dir ./src/components/emails --port 3001",
    "stripe:watch": "stripe listen --forward-to localhost:3000/api/webhooks/stripe"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@mantine/core": "^7.10.1",
    "@mantine/form": "^7.10.1",
    "@mantine/hooks": "^7.10.1",
    "@mantine/modals": "^7.10.1",
    "@mantine/notifications": "^7.10.1",
    "@mantine/nprogress": "^7.10.1",
    "@prisma/client": "^5.15.0",
    "@react-email/components": "0.0.19",
    "@react-email/render": "^0.0.15",
    "@tabler/icons-react": "^3.5.0",
    "bcrypt": "^5.1.1",
    "dotenv-cli": "^7.4.2",
    "micro": "^10.0.1",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.13",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "2.1.4",
    "stripe": "^15.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.3.1",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
